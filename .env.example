# NocoDB Configuration
NC_DB="pg://nocouser:yoursecurepassword@postgres_db:5432/nocodb_starter"
NC_AUTH_JWT_SECRET="replace_this_with_a_very_strong_random_string_at_least_32_chars_long"
NC_PUBLIC_URL="http://localhost:8080"
NC_ADMIN_EMAIL="admin@example.com"
NC_ADMIN_PASSWORD="ReplaceThisPassword1!" # Must be >=8 chars, 1 uppercase, 1 number, 1 special char

# Host port for NocoDB (optional, defaults to 8080 in docker-compose if not set)
NocoDB_HOST_PORT=8080

# PostgreSQL Configuration (must match NC_DB and postgres_db service env vars)
POSTGRES_USER="nocouser"
POSTGRES_PASSWORD="yoursecurepassword"
POSTGRES_DB="nocodb_starter"

# Host port for PostgreSQL (optional, for accessing DB from host, defaults to 5432 in docker-compose if not set)
POSTGRES_HOST_PORT=5432

# Optional: Telemetry (uncomment to disable)
# NC_DISABLE_TELE="true"

# Optional: Redis URL (uncomment and ensure redis_cache service is enabled in docker-compose.yml)
# NC_REDIS_URL="redis://redis_cache:6379"
